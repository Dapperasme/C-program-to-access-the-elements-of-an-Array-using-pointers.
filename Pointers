#include <iostream>
using namespace std;
 
int main()
{
  
   int arry[5]; //Declaration
   cout << "Enter elements: "; //user input 
   for(int i = 0; i < 5; ++i)
   cin >> arry[i];
   cout << "You entered: "; // Output 
   for(int i = 0; i < 5; ++i)
      cout << endl << *(arry + i);
 
   return 0;
}
